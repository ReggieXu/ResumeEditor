<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.udayton.resumeeditor">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/resumeicon3"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".modelActivity.BasicInfoEditActivity"/>
        <activity android:name=".modelActivity.ExperienceEditActivity"/>
        <activity android:name=".modelActivity.EducationEditActivity"/>
        <activity android:name=".modelActivity.ProjectEditActivity"/>


    </application>

</manifest>